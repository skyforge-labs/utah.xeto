// Utah Abstract points

// Air Temperatures
UtahAirTemp: Air & Temp <abstract> {
    doc: "Air Temperature"
    }

UtahAirTempPoint: UtahAirTemp & NumberPoint & SensorPoint <abstract> {
    doc: "Air Temperature Sensor"
    }
 
 UtahAirTempSpPoint: UtahAirTemp & NumberPoint & SpPoint <abstract> {
    doc: "Air Temperature Setpoint"
    }
  
UtahAirTempEntering: UtahAirTempPoint <abstract> {
    entering
    doc: "Air Temperature entering a device"
    color: Str "lightblue"
    }

UtahAirTempLeaving: UtahAirTempPoint <abstract> {
    leaving
    doc: "Air Temperature leaving a device"
    color: Str "blue"
    }

UtahAirTempLeavingSp: UtahAirTempSpPoint <abstract> {
    leaving
    doc: "Air Temperature leaving a device setpoint"
    color: Str "green"
    }

