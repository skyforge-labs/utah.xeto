UtahAirTempSupply: UtahAirTempPoint <abstract> {
    supply
    doc: "Air Temperature supply coming from another device"
    color: Str "purple"
    }

UtahAirTempReturn: UtahAirTempPoint <abstract> {
    return
    doc: "Air Temperature returning to air handling equipment"
    color: Str "orange"
    }

UtahAirTempMixed: UtahAirTempPoint <abstract> {
    mixed
    doc: "Air Temperature mixed within a device"
    color: Str "purple"
    }

UtahAirTempOutside: UtahAirTempPoint <abstract> {
    outside
    doc: "Ambient outside air temperature - dry bulb"
    color: Str "aqua"
    }

